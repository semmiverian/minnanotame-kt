<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="id.semmi.minnanotame">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".DetailLectuer"
                android:label="@string/title_activity_detail_lectuer"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".KosakataActivity"
                android:label="@string/title_activity_kosakata"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".MateriActivity">
        </activity>
        <activity android:name=".ExcerciseActivity">
        </activity>
    </application>

</manifest>